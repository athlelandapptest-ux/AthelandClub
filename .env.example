# Athleland Club Environment Configuration
# Copy this file to .env.local and update the values

# Database Configuration
DATABASE_URL=your_neon_database_url_here

# Feature Flags - Enable Neon database features gradually
# Set to "true" to enable database storage for each feature
USE_NEON_FOR_EVENTS=false
USE_NEON_FOR_REGISTRATIONS=false
USE_NEON_FOR_SPONSORSHIP=false
USE_NEON_FOR_PROGRAMS=false
USE_NEON_FOR_TEMPLATES=true
USE_NEON_FOR_CLASSES=true

# File Upload Configuration
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Other configurations can go here...
